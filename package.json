  "description": "Simple CLI utility to generate images from OpenAI gpt-model-1.",
  "main": "./dist/index.js",
  "type": "module",
  "scripts": {
    "lint-types": "tsc --noEmit",
    "go": "tsx src/index.ts",
    "format": "prettier --write ./src",
    "build": "bun build ./src/index.ts --compile --outfile=gptcli"
