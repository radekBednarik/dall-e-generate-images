  "packageManager": "pnpm@10.7.0",
  "dependencies": {
    "@inquirer/prompts": "^7.5.0",
    "dotenv": "^16.5.0",
    "openai": "^4.98.0",
    "ora": "^8.2.0",
    "prexit": "^2.3.0",
    "terminal-link": "^4.0.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "prettier": "^3.5.3",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
